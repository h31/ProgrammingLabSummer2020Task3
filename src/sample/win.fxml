<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="450.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Label alignment="CENTER" layoutX="80.0" layoutY="150.0" prefHeight="75.0" prefWidth="290.0" text="You Win!" textAlignment="CENTER">
            <font>
                <Font name="Arial Rounded MT Bold" size="64.0" />
            </font>
         <textFill>
            <LinearGradient endX="1.0" endY="1.0" startX="1.0">
               <stops>
                  <Stop color="#10ff00" />
                  <Stop color="#ffe100" offset="0.9961832061068702" />
                  <Stop color="#ffe100" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
        </Label>
        <Button fx:id="tryAgain" alignment="CENTER" contentDisplay="CENTER" layoutX="95.0" layoutY="300.0" mnemonicParsing="false" onAction="#tryAgain" prefHeight="35.0" prefWidth="100.0" style="-fx-background-color: #84776e;" text="Try again" textFill="#faf8ef">
            <font>
                <Font name="Arial Rounded MT Bold" size="16.0" />
            </font>
        </Button>
      <Button fx:id="exit" alignment="CENTER" contentDisplay="CENTER" layoutX="255.0" layoutY="300.0" mnemonicParsing="false" onAction="#exit" prefHeight="35.0" prefWidth="100.0" style="-fx-background-color: #84776e;" text="Exit" textFill="#faf8ef">
         <font>
            <Font name="Arial Rounded MT Bold" size="16.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
