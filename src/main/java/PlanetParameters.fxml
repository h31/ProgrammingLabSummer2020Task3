<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="427.0" prefWidth="456.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ControllerOfThePlanet">
   <Text layoutX="85.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0"
         text="Configure the properties of the planet">
      <font>
         <Font size="16.0"/>
      </font>
   </Text>
   <TextField fx:id="namePl" layoutX="220.0" layoutY="121.0"/>
   <TextField fx:id="weightPl" layoutX="220.0" layoutY="193.0" promptText="kg"/>
   <Text layoutX="87.0" layoutY="140.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name"/>
   <Text layoutX="87.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Color"/>
   <Text layoutX="87.0" layoutY="212.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Weight"/>
   <Text layoutX="87.0" layoutY="286.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Perihelion"/>
   <ColorPicker fx:id="colorPl" layoutX="220.0" layoutY="156.0" prefHeight="29.0" prefWidth="162.0"/>
   <TextField fx:id="perihelionPl" layoutX="220.0" layoutY="267.0" promptText="km"/>
   <TextField fx:id="radiusPl" layoutX="220.0" layoutY="230.0" promptText="km"/>
   <Text layoutX="87.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Radius"/>
   <TextField fx:id="positionPl" layoutX="220.0" layoutY="304.0" promptText="\%"/>
   <Text layoutX="87.0" layoutY="323.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Position"/>
   <Button fx:id="applyPl" layoutX="200.0" layoutY="359.0" mnemonicParsing="false" text="APPLY"/>
</AnchorPane>
