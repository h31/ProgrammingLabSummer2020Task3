<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="288.0" prefWidth="456.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ControllerOfTheMain">
   <Text layoutX="26.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0"
         text="Configuring basic parameters of the planetary system">
      <font>
         <Font size="16.0"/>
      </font>
   </Text>
   <TextField id="weight" fx:id="weight" layoutX="220.0" layoutY="121.0" promptText="kg * 10^11"/>
   <TextField id="starRadius" fx:id="radius" layoutX="220.0" layoutY="157.0" promptText="*10^4 km"/>
   <Slider fx:id="number" blockIncrement="1.0" cache="true" layoutX="220.0" layoutY="193.0" majorTickUnit="1.0"
           max="10.0" min="1.0" minorTickCount="0" prefHeight="40.0" prefWidth="162.0" showTickLabels="true"
           showTickMarks="true" snapToTicks="true" value="1.0"/>
   <Text layoutX="87.0" layoutY="140.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Weight of star"/>
   <Text layoutX="87.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Radius of star"/>
   <Text layoutX="87.0" layoutY="212.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Number of planets"/>
   <Button fx:id="apply" layoutX="207.0" layoutY="235.0" mnemonicParsing="false" text="APPLY"/>
</AnchorPane>
